#: extension.js:108
msgid "Text will be here"
msgstr "Testua hemen azalduko da"

#: extension.js:181
msgid "Type here to search..."
msgstr "Idatzi hemen bilatzeko..."

#: extension.js:250 prefs.js:220
msgid "Private mode"
msgstr "Modu pribatua"

#: extension.js:264 prefs.js:222
msgid "Clear history"
msgstr "Garbitu historia"

#: extension.js:276
msgid "Settings"
msgstr "Ezarpenak"

#: extension.js:299
msgid "Clipboard is empty"
msgstr "Arbela hutsik dago"

#: extension.js:571
msgid "Clear all?"
msgstr "Guztia garbitu?"

#: extension.js:572
msgid "Are you sure you want to delete all clipboard items?"
msgstr "Ziur arbeleko elementu guztiak kendu nahi dituzula?"

#: extension.js:573
msgid "This operation cannot be undone."
msgstr "Ezin da eragiketa hau desegin."

#: extension.js:575
msgid "Clear"
msgstr "Garbitu"

#: extension.js:575 extension.js:740
msgid "Cancel"
msgstr "Utzi"

#: extension.js:588
msgid "Clipboard history cleared"
msgstr "Arbelaren historia garbituta"

#: extension.js:739
msgid "Copied to clipboard"
msgstr "Arbelera kopiatuta"

#: prefs.js:30
msgid "History Size"
msgstr "Historiaren tamaina"

#: prefs.js:39
msgid "Preview Size (characters)"
msgstr "Aurrebistaren tamaina (karaktereak)"

#: prefs.js:48
msgid "Max cache file size (MB)"
msgstr "Cache fitxategiaren gehienezko tamaina (MB)"

#: prefs.js:57
msgid "Number of characters in top bar"
msgstr "Goiko barrako karaktere kopurua"

#: prefs.js:66
msgid "What to show in top bar"
msgstr "Zer erakutsi goiko barran"

#: prefs.js:71
msgid "Remove down arrow in top bar"
msgstr "Kendu beheranzko gezia goiko barran"

#: prefs.js:75
msgid "Cache only pinned items"
msgstr "Cacheratu ainguratutako elementuak soilik"

#: prefs.js:79
msgid "Show notification on copy"
msgstr "Erakutsi jakinarazpena kopiatzean"

#: prefs.js:83
msgid "Show notification on cycle"
msgstr "Erakutsi jakinarazpena nabigatzean"

#: prefs.js:87
msgid "Show confirmation on Clear History"
msgstr "Eskatu berrespena historia garbitzerakoan"

#: prefs.js:91
msgid "Remove whitespace around text"
msgstr "Kendu testuaren inguruko hutsuneak"

#: prefs.js:95
msgid "Move item to the top after selection"
msgstr "Eraman elementua goraino hautatu ostean"

#: prefs.js:99
msgid "Keep selected entry after Clear History"
msgstr "Gorde hautatutako elementua historia garbitu ondoren"

#: prefs.js:103
msgid "Show paste buttons"
msgstr "Erakutsi itsasteko botoiak"

#: prefs.js:104
msgid "Adds a paste button to each entry that lets you paste it directly"
msgstr ""
"Sarrera bakoitzaren alboan itsasteko botoi bat gehitzen du, zuzenean "
"itsasteko"

#: prefs.js:108
msgid "Place the pinned section on the bottom"
msgstr "Kokatu ainguratutakoen atala behealdean"

#: prefs.js:109
msgid "Requires restarting the extension"
msgstr "Hedapena berrabiarazi behar da"

#: prefs.js:113
msgid "Clear clipboard history on system reboot"
msgstr "Garbitu arbelaren historia sistema berrabiaraztean"

#: prefs.js:117
msgid "Paste on select"
msgstr "Itsatsi hautatzean"

#: prefs.js:121
msgid "Cache images"
msgstr "Cacheratu irudiak"

#: prefs.js:126
msgid "Excluded Apps"
msgstr "Salbuetsitako aplikazioak"

#: prefs.js:127
msgid "Content copied will not be saved while these apps are in focus"
msgstr "Aplikazio hauek fokua dutenean kopiatutako edukia ez da gordeko"

#: prefs.js:146
msgid "UI"
msgstr "Erabiltzaile interfazea"

#: prefs.js:147
msgid "Behavior"
msgstr "Portaera"

#: prefs.js:148
msgid "Exclusion"
msgstr "Salbuespena"

#: prefs.js:149
msgid "Limits"
msgstr "Mugak"

#: prefs.js:150
msgid "Topbar"
msgstr "Goiko barra"

#: prefs.js:151
msgid "Notifications"
msgstr "Jakinarazpenak"

#: prefs.js:152
msgid "Shortcuts"
msgstr "Laster-teklak"

#: prefs.js:207
msgid "Icon"
msgstr "Ikonoa"

#: prefs.js:208
msgid "Clipboard Content"
msgstr "Arbelaren edukia"

#: prefs.js:209
msgid "Both"
msgstr "Biak"

#: prefs.js:210
msgid "Neither"
msgstr "Bat ere ez"

#: prefs.js:221
msgid "Toggle the menu"
msgstr "Erakutsi/ezkutatu menua"

#: prefs.js:223
msgid "Previous entry"
msgstr "Aurreko sarrera"

#: prefs.js:224
msgid "Next entry"
msgstr "Hurrengo sarrera"

#: prefs.js:229
msgid "Enable shortcuts"
msgstr "Gaitu laster-teklak"

#: prefs.js:254
msgid "Disabled"
msgstr "Ezgaituta"

#: prefs.js:263
msgid "Enter shortcut"
msgstr "Sartu laster-tekla"

#: prefs.js:345
msgid "Window class name, e.g. \"KeePassXC\""
msgstr "Leihoaren klase-izena, adb. \"KeePassXC\""

